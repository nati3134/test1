pipeline{
    agent any
    stages {
        stage("clean") {
            steps {
                deleteDir()
            }
        }
        stage('clone repo'){
            steps{
                bat "git clone https://github.com/nati3134/test1"
            }
        }
        stage(docker_build){
            steps{
                bat  "docker build . -t liran -f test1/Dockerfile"
            }
        }
    }
}

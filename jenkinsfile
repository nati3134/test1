pipeline{
    agent any
    stages {
        stage("clean") {
            steps {
                deleteDir()
            }
        }
        stage('clone repo'){
            steps{
                bat "git clone https://github.com/nati3134/test1"
            }
        }
        stage('end of pipe'){
            steps{
                echo 'end of pipeline'
            }
            
        }
        stage('docker build'){
            steps{
                scrip
                    bat 'docker build -t my-app-01 .'
                    
            }
            }
            
        }
        }
    }
